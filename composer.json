{
	"name": "kotygor/opencart-sync-relations",
	"descripiton": "Створення відношень між унікальними ідентифікаторами зовнішнього ресурсу і опенкарту",
	"type": "opencart-extension",
	"minimum-stability": "dev",
	"extra": {
		"src-dir" : "upload",
		"mappings" : [
			"admin/model/tool/jd_tools/relations.php",
			"system/jd_tools/relation.php"
		],
		"turned-off-installers" : {
			"php" : "install.php"
		},
		"opencart-install-dir": "opencart",
		"opencart-storage-dir": "opencart-storage"
	},
	"require": {
		"kotygor/opencart-extension-installer": "dev-master",
		"kotygor/opencart": "dev-fcb"
	},
	"require-dev": {
		"kotygor/mylog": "v2.x-dev"
	},
	"repositories": [
		{
			"type": "git",
			"url": "https://github.com/kotygor/opencart-extension-installer"
		},
		{
			"type": "git",
			"url": "https://github.com/kotygor/opencart"
		},
		{
			"type": "git",
			"url": "https://github.com/kotygor/mylog"
		},
		{
			"type": "git",
			"url": "https://github.com/kotygor/opencart-core-installer"
		}
	]
}
